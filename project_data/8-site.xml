<config xmlns="http://tail-f.com/ns/config/1.0">
  <vnf-manager xmlns="http://example.com/ftdv-ngfw">
  <site>
    <name>bofa-ngfw</name>
    <tenant>admin</tenant>
    <datacenter-name>NGFW-BOALAB</datacenter-name>
    <cluster-name>Cluster</cluster-name>
    <datastore-name>cpoc_share_4TB_infra</datastore-name>
    <networks>
      <network>
        <name>Inside</name>
        <network>172.16.2.0</network>
        <netmask>255.255.255.0</netmask>
        <prefix>24</prefix>
        <gateway-ip-address>172.16.2.1</gateway-ip-address>
        <vlan-id>162</vlan-id>
        <resource-pool>
          <subnet>
            <address>172.16.2.234</address>
            <cidrmask>30</cidrmask>
          </subnet>
        </resource-pool>
        <VMware-name>NGFW_PRESENTATION-2</VMware-name>
        <VMware-switch>vSwitch0</VMware-switch>
        <intelligent-traffic-director-side xmlns="http://com/example/intelligenttrafficdirector">inside</intelligent-traffic-director-side>
      </network>
      <network>
        <name>Inside-SRIOV</name>
        <network>172.16.2.0</network>
        <netmask>255.255.255.0</netmask>
        <prefix>24</prefix>
        <gateway-ip-address>172.16.2.1</gateway-ip-address>
        <vlan-id>162</vlan-id>
        <resource-pool>
          <subnet>
            <address>172.16.2.234</address>
            <cidrmask>30</cidrmask>
          </subnet>
        </resource-pool>
        <VMware-name>SRIOV-INTERNAL</VMware-name>
        <VMware-switch>vSwitch0</VMware-switch>
        <intelligent-traffic-director-side xmlns="http://com/example/intelligenttrafficdirector">inside</intelligent-traffic-director-side>
      </network>
      <network>
        <name>Inside-Servers</name>
        <network>10.230.230.0</network>
        <netmask>255.255.255.0</netmask>
        <prefix>24</prefix>
        <gateway-ip-address>172.16.2.1</gateway-ip-address>
        <vlan-id>161</vlan-id>
        <ftd-network/>
      </network>
      <network>
        <name>Management</name>
        <network>172.26.32.192</network>
        <netmask>255.255.255.192</netmask>
        <prefix>26</prefix>
        <gateway-ip-address>172.26.32.193</gateway-ip-address>
        <vlan-id>1</vlan-id>
        <resource-pool>
          <subnet>
            <address>172.26.32.234</address>
            <cidrmask>30</cidrmask>
          </subnet>
        </resource-pool>
        <dns-ip-address>172.26.32.218</dns-ip-address>
        <VMware-name>Cisco Network</VMware-name>
        <VMware-switch>vSwitch0</VMware-switch>
      </network>
      <network>
        <name>Outside</name>
        <network>172.16.1.0</network>
        <netmask>255.255.255.0</netmask>
        <prefix>24</prefix>
        <gateway-ip-address>172.16.1.1</gateway-ip-address>
        <vlan-id>161</vlan-id>
        <resource-pool>
          <subnet>
            <address>172.16.1.232</address>
            <cidrmask>30</cidrmask>
          </subnet>
        </resource-pool>
        <VMware-name>NGFW_OUTSIDE2</VMware-name>
        <VMware-switch>vSwitch0</VMware-switch>
        <intelligent-traffic-director-side xmlns="http://com/example/intelligenttrafficdirector">outside</intelligent-traffic-director-side>
      </network>
      <network>
        <name>Outside-Clients</name>
        <network>10.200.200.0</network>
        <netmask>255.255.255.0</netmask>
        <prefix>24</prefix>
        <gateway-ip-address>172.16.1.1</gateway-ip-address>
        <vlan-id>162</vlan-id>
        <ftd-network/>
      </network>
      <network>
        <name>Outside-SRIOV</name>
        <network>172.16.1.0</network>
        <netmask>255.255.255.0</netmask>
        <prefix>24</prefix>
        <gateway-ip-address>172.16.1.1</gateway-ip-address>
        <vlan-id>161</vlan-id>
        <resource-pool>
          <subnet>
            <address>172.16.1.232</address>
            <cidrmask>30</cidrmask>
          </subnet>
        </resource-pool>
        <VMware-name>SRIOV-External</VMware-name>
        <VMware-switch>vSwitch0</VMware-switch>
        <intelligent-traffic-director-side xmlns="http://com/example/intelligenttrafficdirector">outside</intelligent-traffic-director-side>
      </network>
    </networks>
    <elastic-services-controller>ESC</elastic-services-controller>
    <managers>
      <manager>
        <name>fmc-virtual</name>
      </manager>
    </managers>
    <intelligent-traffic-director xmlns="http://com/example/intelligenttrafficdirector">
      <devices>
        <device>kansas</device>
        <side>inside</side>
      </devices>
      <devices>
        <device>oaklahoma</device>
        <side>outside</side>
      </devices>
    </intelligent-traffic-director>
  </site>
  </vnf-manager>
</config>
