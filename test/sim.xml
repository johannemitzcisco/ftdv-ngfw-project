<config xmlns="http://tail-f.com/ns/config/1.0">
  <nfv xmlns="urn:etsi:nfv:yang:etsi-nfv-descriptors">
  <vnf-info xmlns="http://cisco.com/ns/nso/cfp/cisco-etsi-nfvo">
    <name>t</name>
    <username>admin</username>
    <vnfd>Cisco-FTD</vnfd>
    <vnfd-flavour>Small</vnfd-flavour>
    <instantiation-level>Scalable</instantiation-level>
    <vnfm>ESC</vnfm>
    <vnfm-type>netconf</vnfm-type>
    <vim-type>vcenter</vim-type>
    <vdu>
      <id>Small</id>
      <image-name>FTDv-6.5.0-115-Template</image-name>
      <bootup-time>1800</bootup-time>
      <recovery-wait-time>1800</recovery-wait-time>
      <netconf-volume>
        <name>cpoc_share_4TB_infra</name>
        <id>1</id>
      </netconf-volume>
      <esc-device-template>
        <name>ftdv-fmc-managed-esc-vmware-simple-monitoring</name>
        <template-id>ftdv-fmc-managed-esc-vmware-simple-monitor</template-id>
        <variable>
          <name>CLUSTER_NAME</name>
          <value>Cluster</value>
        </variable>
        <variable>
          <name>DATACENTER_NAME</name>
          <value>NGFW-BOALAB</value>
        </variable>
        <variable>
          <name>DEPLOY_VNF_PASSWORD</name>
          <value>Adm!n123</value>
        </variable>
        <variable>
          <name>DNS_SERVER_IP</name>
          <value>172.26.32.218</value>
        </variable>
        <variable>
          <name>FMC_IP_ADDRESS</name>
          <value>172.26.32.250</value>
        </variable>
        <variable>
          <name>FMC_REGISTRATION_KEY</name>
          <value>0123456789</value>
        </variable>
        <variable>
          <name>GATEWAY_IP_ADDRESS</name>
          <value>172.26.32.193</value>
        </variable>
        <variable>
          <name>INSIDE_NETWORK</name>
          <value>NGFW_Inside</value>
        </variable>
        <variable>
          <name>MGMT_IPADDRESS</name>
          <value>172.26.32.235</value>
        </variable>
        <variable>
          <name>MGMT_IP_NETMASK</name>
          <value>255.255.255.193</value>
        </variable>
        <variable>
          <name>MGMT_NETWORK</name>
          <value>Cisco Network</value>
        </variable>
        <variable>
          <name>MONITORS_ENABLED</name>
          <value>false</value>
        </variable>
        <variable>
          <name>MONITOR_PASSWORD</name>
          <value>C!sco123</value>
        </variable>
        <variable>
          <name>MONITOR_USERNAME</name>
          <value>C!sco123</value>
        </variable>
        <variable>
          <name>OUTSIDE_NETWORK</name>
          <value>NGFW_Outside</value>
        </variable>
        <variable>
          <name>SCALEDOWN_THRESHOLD</name>
          <value>2</value>
        </variable>
        <variable>
          <name>SCALEUP_THRESHOLD</name>
          <value>2</value>
        </variable>
      </esc-device-template>
      <resource-allocation>
        <host-placement>172.26.32.200</host-placement>
      </resource-allocation>
      <authgroup>ftd</authgroup>
      <host-key-verification-type>none</host-key-verification-type>
      <internal-connection-point>
        <id>Inside</id>
        <connection-point-address>
          <netconf-parameters>
            <address>10.10.10.100</address>
            <ip-address-range>
              <start>10.10.10.100</start>
              <end>10.10.10.101</end>
            </ip-address-range>
            <netmask>255.255.255.0</netmask>
          </netconf-parameters>
        </connection-point-address>
      </internal-connection-point>
      <internal-connection-point>
        <id>Management</id>
        <connection-point-address>
          <netconf-parameters>
            <address>172.26.32.235</address>
            <ip-address-range>
              <start>172.26.32.235</start>
              <end>172.26.32.236</end>
            </ip-address-range>
            <netmask>255.255.255.193</netmask>
            <gateway>172.26.32.193</gateway>
          </netconf-parameters>
        </connection-point-address>
      </internal-connection-point>
      <internal-connection-point>
        <id>Outside</id>
        <connection-point-address>
          <netconf-parameters>
            <address>10.20.10.100</address>
            <ip-address-range>
              <start>10.20.10.100</start>
              <end>10.20.10.101</end>
            </ip-address-range>
            <netmask>255.255.255.0</netmask>
          </netconf-parameters>
        </connection-point-address>
      </internal-connection-point>
    </vdu>
    <vnfd-connection-point>
      <id>Small-Inside</id>
      <network-name>NGFW_Inside</network-name>
    </vnfd-connection-point>
    <vnfd-connection-point>
      <id>Small-Management</id>
      <network-name>Cisco Network</network-name>
    </vnfd-connection-point>
    <vnfd-connection-point>
      <id>Small-Outside</id>
      <network-name>NGFW_Outside</network-name>
    </vnfd-connection-point>
  </vnf-info>
  </nfv>
</config>
